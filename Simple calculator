#include <iostream>

int main() {
  char op;
  int num1, num2;
  std::cout << "Enter operator (+, -, *, /): ";
  std::cin >> op;
  std::cout << "Enter two numbers: ";
  std::cin >> num1 >> num2;

  switch (op) {
    case '+': std::cout << num1 + num2 << std::endl; break;
    case '-': std::cout << num1 - num2 << std::endl; break;
    case '*': std::cout << num1 * num2 << std::endl; break;
    case '/': std::cout << num1 / num2 << std::endl; break;
    default: std::cout << "Invalid operator." << std::endl;
  }
  return 0;
}
